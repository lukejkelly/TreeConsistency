# unrooted tree prior and associated mcmc moves

topology ~ dnUniformTopology(taxa)
moves.append( mvNNI(topology) )
moves.append( mvSPR(topology) )

num_taxa <- alleles.ntaxa()
num_branches <- 2 * num_taxa - 3
for (i in 1:num_branches) {
    brlens[i] ~ dnExponential(1)
    moves.append( mvScale(brlens[i]) )
}

tree := treeAssembly(topology, brlens)
